{
  "version": 2,
  "builds": [
    { "src": "pages/**/*.js", "use": "@vercel/node" }
  ],
  "env": {
    "ADMIN_ID": "your_admin_telegram_id",  // (মামুন) এখানে তোমার টেলিগ্রাম এডমিন আইডি বসাবে
    "BOT_API_TOKEN": "your_telegram_bot_api_token"  // (মামুন) এখানে টেলিগ্রাম বটের API টোকেন বসাবে
  }
}